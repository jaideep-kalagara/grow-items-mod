{
  "schemaVersion": 1,
  "id": "growitems",
  "version": "${version}",
  "name": "Grow Items",
  "description": "A mod that lets you grow items with a new block!",
  "authors": ["SuperGamer10920"],
  "contact": {"homepage": "https://github.com/jaideep-kalagara/grow-items-mod/"},
  "license": "MIT",
  "icon": "assets/growitems/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.supergamer.growitems.client.GrowItemsDataGenerator"
    ],
    "client": [
      "net.supergamer.growitems.client.GrowItemsClient"
    ],
    "main": [
      "net.supergamer.growitems.GrowItems"
    ]
  },
  "mixins": [
    "growitems.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
